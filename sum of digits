import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        
        int n=sc.nextInt();
        int b=0;
        int sum=0;
        if (n<0){
            n=-n;
        }
        while(n>0){
            b=n%10;
            n = n/10; 
            sum+=b;
            
        }
        System.out.println(sum);
        }
    }
